{
  "name": "gotql",
  "version": "1.8.4",
  "engines": {
    "node": ">=8"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npm run test:jest",
    "test:jest": "npm run build:clean && jest",
    "test:ava": "npm run build:clean && ava --color -v -T 10000",
    "prepare": "npm run build:clean",
    "lint": "standard -v ./src",
    "build": "tsc",
    "build:watch": "tsc -w",
    "clean": "rm -rf dist",
    "build:clean": "npm run clean && npm run build",
    "lint:fix": "standard --fix"
  },
  "description": "A GraphQL query framework for serverside apps",
  "main": "dist/index.js",
  "repository": "https://github.com/khaosdoctor/gotql.git",
  "bugs": {
    "url": "https://github.com/khaosdoctor/gotql/issues"
  },
  "pre-commit": [
    "lint"
  ],
  "pre-push": [
    "lint",
    "build"
  ],
  "ava": {
    "files": [
      "tests/**/*.test.js"
    ]
  },
  "author": "Lucas Santos <lhs.santoss@gmail.com>",
  "license": "MIT",
  "keywords": [
    "graphql",
    "query",
    "framework",
    "serverside",
    "graph",
    "post"
  ],
  "private": false,
  "dependencies": {
    "debug": "^4.1.1",
    "got": "^9.6.0",
    "prepend-http": "^3.0.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/got": "^9.6.10",
    "@types/jest": "^26.0.4",
    "@types/node": "^13.13.4",
    "ava": "^2.4.0",
    "codecov": "^3.5.0",
    "concurrently": "^4.1.2",
    "conduct": "^2.3.1",
    "jest": "^26.1.0",
    "nyc": "^14.1.1",
    "pre-push": "^0.1.1",
    "sinon": "^7.5.0",
    "standard": "^14.3.3",
    "ts-jest": "^26.1.2",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.8.3"
  }
}
